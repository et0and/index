{
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "semi": false,
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "es5",
    "proseWrap": "always",
    "arrowParens": "always",
    "tabWidth": 4,
    "useTabs": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "bracketSameLine": false
}